


<h1>Account Login</h1>

<% Helpers.flash_types.select {|type| flash.has?(type)}.each do |type| %>
  <div class="flash <%= type %>">
    <strong><%= flash[type] %></strong>
  </div>
<% end %>

<form method="POST" action="/login">

  <label><p>Username:</p><input type = "text" name="username" id="username"><br><br>
  <label><p>Password:</p><input type="text" name="password" id="password"><br><br>
  <input type="submit" value="submit">

</form>
